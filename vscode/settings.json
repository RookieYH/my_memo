// 将设置放入此文件中以覆盖默认设置
{
    "files.exclude": {
        "target/": true,
        ".settings/": true,
        ".project": true,
        ".classpath": true,
        "*.iml": true,
        "*.iws": true,
        ".idea/": true,
        "out/": true,
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true
    },
    "workbench.iconTheme": "vscode-icons",
    "extensions.autoUpdate": true,
    "workbench.welcome.enabled": true
}